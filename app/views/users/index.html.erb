<div class="page-header">
  <h1>All Users</h1>
</div>

<table class="table table-bordered table-striped">
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Display Name</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.email, user %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.display_name %></td>
  </tr>
<% end %>
</table>

<br />

<a href="#registerModal" role="button" data-toggle="modal">New User</a>